<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_hasil_servqual3"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.ari.saidata.hasil_servqual2">

    <HorizontalScrollView
        android:layout_width="match_parent"
        android:layout_height="430dp">

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <!-- child views in here -->

                <TableLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/tablelayoutid">

                    <TableRow
                        android:id="@+id/rowlay"
                        android:background="@android:color/holo_blue_light"
                        android:padding="5dp">


                        <TextView
                            android:layout_width="200dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:text="Hasil A"
                            android:textStyle="normal|bold"
                            android:textAlignment="center"/>

                        <TextView
                            android:layout_width="200dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:text="Hasil A2"
                            android:textStyle="normal|bold"
                            android:textAlignment="center"/>

                        <TextView
                            android:layout_width="200dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:text="Nilai B1"
                            android:textStyle="normal|bold"
                            android:textAlignment="center"/>

                        <TextView
                            android:layout_width="200dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:text="Nilai B2"
                            android:textStyle="normal|bold"
                            android:textAlignment="center"/>

                        <TextView
                            android:layout_width="200dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:text="Nilai B3"
                            android:textStyle="normal|bold"
                            android:textAlignment="center"/>

                        <TextView
                            android:layout_width="200dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:text="Nilai B4"
                            android:textStyle="normal|bold"
                            android:textAlignment="center"/>

                        <TextView
                            android:layout_width="200dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:text="Nilai B5"
                            android:textStyle="normal|bold"
                            android:textAlignment="center"/>

                        <TextView
                            android:layout_width="200dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:text="Nilai CH"
                            android:textStyle="normal|bold"
                            android:textAlignment="center"/>

                        <TextView
                            android:layout_width="200dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:text="Nilai DH"
                            android:textStyle="normal|bold"
                            android:textAlignment="center"/>

                        <TextView
                            android:layout_width="200dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:text="Hasil Servqual"
                            android:textStyle="normal|bold"
                            android:textAlignment="center"/>


                    </TableRow>



                </TableLayout>

            </LinearLayout>

        </ScrollView>

    </HorizontalScrollView>

</RelativeLayout>
